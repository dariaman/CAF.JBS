@model IEnumerable<CAF.JBS.Models.CardIssuerBankModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Type)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Prefix)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Description)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.acquirer_bank_id)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prefix)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.acquirer_bank_id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.card_issuer_bank_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.card_issuer_bank_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.card_issuer_bank_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
